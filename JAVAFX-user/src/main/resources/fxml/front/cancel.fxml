<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20" style="-fx-background-color: #f0f0f0;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label style="-fx-font-size: 24px; -fx-font-weight: bold; -fx-text-fill: red;" text="Payment Canceled">
            <padding>
                <Insets top="50" />
            </padding>
        </Label>
        <Label style="-fx-font-size: 16px;" text="Your payment was canceled. You can try again or return to the product list." />
    </children>
</VBox>